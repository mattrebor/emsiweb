<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 2.1//EN" "http://tiles.apache.org/dtds/tiles-config_2_1.dtd">
<tiles-definitions>
	
	<definition name="semistatic/*/*" extends="default-two-col">
		<put-attribute name="menu-content" value="/WEB-INF/views/semistatic/{1}/left-nav.jspx" cascade="true"/>
		<put-attribute name="body-content" value="/WEB-INF/views/semistatic/{1}/{2}.jspx" cascade="true"/>
	</definition>
	
	<definition name="semistatic/*/*/*" extends="default-two-col">
		<put-attribute name="menu-content" value="/WEB-INF/views/semistatic/{1}/left-nav.jspx" cascade="true"/>
		<put-attribute name="body-content" value="/WEB-INF/views/semistatic/{1}/{2}/{3}.jspx" cascade="true"/>
		<put-attribute name="interior-nav" value="/WEB-INF/views/semistatic/{1}/{2}/interior_nav.jspx" cascade="true"/>
	</definition>
</tiles-definitions>